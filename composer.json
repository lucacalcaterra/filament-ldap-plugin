{
    "name": "lucacalcaterra/filament-ldap-plugin",
    "description": "This is my package filament-ldap-plugin",
    "keywords": [
        "lucacalcaterra",
        "laravel",
        "filament-ldap-plugin"
    ],
    "homepage": "https://github.com/lucacalcaterra/filament-ldap-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Calcaterra",
            "email": "calcaterra.luca@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Lucacalcaterra\\FilamentLdapPlugin\\": "src",
            "Lucacalcaterra\\FilamentLdapPlugin\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lucacalcaterra\\FilamentLdapPlugin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucacalcaterra\\FilamentLdapPlugin\\FilamentLdapPluginServiceProvider"
            ],
            "aliases": {
                "FilamentLdapPlugin": "Lucacalcaterra\\FilamentLdapPlugin\\Facades\\FilamentLdapPlugin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
